#!/usr/bin/env node

'use strict';

// mri interface

const _program = require('commander');
const $interface = require('../service/interface');

_program
    .usage('interface [options]')
    .option('-i, --INTERFACE', '生成const interface文件')
    .parse(process.argv);


/**
 * dev only
 * 自动生成const的接口文件
 */
console.log(`\n\n--=> 自动生成const的接口文件 - - - - - - - - - `);
// $interface.init(_program['INTERFACE']);

