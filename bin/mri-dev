#!/usr/bin/env node

/**
 *  mri dev ${theme}
 *  启动 MRI 项目
 */

'use strict';

const $run = require('../service/run');
const $_project = require('../process/project');

const MRI_ENV = 'dev';

$run.root(() => {
    $run.package(() => {
        $run.upgrade(() => {
            $run.branch('all', () => {
                $_project(MRI_ENV);
            });
        });
    });
});
